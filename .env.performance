# Language settings
LANG=en_US.UTF-8

# Node settings
NVM_BIN=/Users/weg/.nvm/versions/node/v22.17.0/bin

# Performance optimizations for M3 Pro
# Use all available cores (M3 Pro has 12 performance cores)
PARALLEL_JOBS=12
MAKEFLAGS="-j12"
CMAKE_BUILD_PARALLEL_LEVEL=12

# Python optimizations
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
PIP_DISABLE_PIP_VERSION_CHECK=1
PIP_NO_CACHE_DIR=0

# Increase memory limits
NODE_OPTIONS="--max-old-space-size=8192"

# Enable parallel pip installations
PIP_PARALLEL_DOWNLOADS=10

# TensorFlow optimizations for Apple Silicon
TF_ENABLE_ONEDNN_OPTS=1
TF_CPP_MIN_LOG_LEVEL=2

# Cache directories
PIP_CACHE_DIR=/Users/weg/.cache/pip
NEURAL_ENGINE_CACHE_DIR=/Users/weg/.neural-engine-cache
